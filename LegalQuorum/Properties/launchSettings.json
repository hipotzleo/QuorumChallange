{
  "profiles": {
    "LegalQuorum.App": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DataPaths:People": "./data/legislators.csv",
        "DataPaths:Bills": "./data/bills.csv",
        "DataPaths:Votes": "./data/votes.csv",
        "DataPaths:VoteResults": "./data/vote_results.csv",
        "Logging:LogLevel:Default": "Information",
        "Logging:LogLevel:Microsoft.AspNetCore": "Warning",
        "AllowedHosts": "*"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7123;http://localhost:5123"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DataPaths:People": "./data/legislators.csv",
        "DataPaths:Bills": "./data/bills.csv",
        "DataPaths:Votes": "./data/votes.csv",
        "DataPaths:VoteResults": "./data/vote_results.csv",
        "Logging:LogLevel:Default": "Information",
        "Logging:LogLevel:Microsoft.AspNetCore": "Warning",
        "AllowedHosts": "*"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:59279/",
      "sslPort": 44391
    }
  }
}